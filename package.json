{
  "name": "express-server",
  "version": "1.0.0",
  "main": "server.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "nodemon src/server --exec babel-node",
    "start-db": "docker start postgres_db",
    "migrate": "npx prisma migrate dev --preview-feature --skip-generate && npx prisma generate",
    "generate": "npx prisma generate",
    "seed": "nodemon src/db/seed.js --exec babel-node",
    "build": "babel src -d build",
    "serve": "node build/server",
    "test": "jest --watch",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@prisma/client": "2.28.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prisma": "2.28.0"
  }
}
